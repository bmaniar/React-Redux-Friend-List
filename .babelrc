{
    "presets": [
        ["@babel/preset-env"],
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-syntax-dynamic-import",
        ["@babel/plugin-proposal-class-properties", { "loose": true }]
    ],
    "env": {
        "development": {
          "plugins": ["react-hot-loader/babel"]
        }
    }
}